include ./layouts/header.jade

style.
  table, th, td {
  border: 1px solid black;}

div.container-fluid.page-wrap
  div.row
    div.col-md-10.col-md-offset-1
      //h1.Title.text-center #{message} By Program Type
      div.row
        div
          | <br>
          table.table-striped.table-bordered
            thead
            h4  #{message} By Program Type
              tr
                th Class Type &nbsp
                th #{message} &nbsp
            tbody
              each row, i in rows
                tr
                  td
                    =row.programType
                  td
                    =row.average
          | <br>
          | <br>    
                          
include ./layouts/footer.jade
